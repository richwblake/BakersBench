<h1><%= "#{@user.name}'s Recipes" %></h1>

    <% @recipes.each do |recipe| %>
        <h3><%= "Title: #{recipe.title}" %></h3>
        <%= "Description: #{recipe.description}" %><br>
        <%= "Total Time Required: #{recipe.total_prep_time}" %><br>
        <%= "Number of subscribers: #{recipe.subscriptions.count} " %><br>
        <br>
    <% end %>
